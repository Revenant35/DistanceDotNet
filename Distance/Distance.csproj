<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <AssemblyName>DistanceDotNet</AssemblyName>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Version>1.0.0</Version>
        <Authors>d1ggity</Authors>
        <Description>A minimal library for managing and converting between distances.</Description>
        <PackageTags>distance, conversion, units</PackageTags>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/Revenant35/DistanceDotNet.git</RepositoryUrl>
    </PropertyGroup>
</Project>
